	author = {Xu, Duo and Tan, Jonathan C. and Hsu, Chia-Jung and Zhu, Ye},
	month = jun,
	year = {2023},
	note = {arXiv:2304.01670 [astro-ph]},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Instrumentation and Methods for Astrophysics, Computer Science - Machine Learning},
	pages = {146},
