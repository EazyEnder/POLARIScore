	file = {Preprint PDF:C\:\\Users\\Zack\\Zotero\\storage\\DB9WDXBI\\Xu et al. - 2023 - Denoising Diffusion Probabilistic Models to Predict the Density of Molecular Clouds.pdf:application/pdf;Snapshot:C\:\\Users\\Zack\\Zotero\\storage\\JT7D7AD3\\2304.html:text/html},
